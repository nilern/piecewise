%%

%eop EOF

(* %pos declares the type of positions for terminals.
   Each symbol has an associated left and right position. *)

%pos int

%term ID of string | EOF
%nonterm EXP of CST.expr | START of CST.expr

%name Pcws

%noshift EOF
%value ID ("bogus")
%verbose

%%

(* the parser returns the value associated with the expression *)

START : EXP (EXP)

EXP : ID (CST.Var (Var.Lex (Name.Plain ID)))
